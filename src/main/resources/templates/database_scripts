CREATE TABLE person(
    `id` int NOT NULL AUTO_INCREMENT,
    `first_name` varchar(50),
    `last_name` varchar(50),
    `phone_number` varchar(50),
    `email` varchar(50),
    `age` int,
    `address` varchar(100),

    PRIMARY KEY (`id`),
    UNIQUE KEY `id_UNIQUE` (`id`)

) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;